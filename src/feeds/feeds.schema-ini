[DEFAULT/notify]
type = bool
default = True
description = Notify if any feeds have new items

[DEFAULT/auto_update]
type = bool
default = True
description = Update automatically at a configurable interval

[DEFAULT/update_interval]
type = int
default = 5
description = The interval (in minutes) at which to update (3-60)

[DEFAULT/google_token]
type = int
default = 0
description = Used for logging in to Google Reader; do not touch.
